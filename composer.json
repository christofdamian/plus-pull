    "homepage": "https://github.com/christofdamian/plus-pull",
    "license": "MIT",

    "require": {
        "symfony/console": "*",
        "symfony/yaml": "*",
        "knplabs/github-api": "*",
        "phpunit/phpunit": "^7",
        "php-http/guzzle6-adapter": "*"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "psr-0": { "PlusPull\\": "src/" }
    },
    "bin": ["bin/pluspull.php"],
    "authors": [
        {
            "name": "Christof Damian",
            "email": "christof@damian.net"
        }
    ]
}
